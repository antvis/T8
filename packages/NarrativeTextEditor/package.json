{
  "name": "@antv/narrative-text-editor",
  "version": "0.1.0",
  "description": "React component of narrative text editor",
  "keywords": [
    "react component",
    "rich text editor",
    "narrative text"
  ],
  "author": {
    "name": "AntV",
    "url": "https://antv.vision/"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/src/index.d.ts",
  "unpkg": "dist/narrative-text-editor.min.js",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./src",
    "test": "jest",
    "build:esm": "rimraf es && FORMAT=esm rollup -c",
    "build:cjs": "rimraf lib && FORMAT=cjs rollup -c",
    "build:umd": "rimraf dist && FORMAT=umd rollup -c",
    "build": "run-s build:cjs build:esm build:umd",
    "watch:esm": "rimraf es && FORMAT=esm rollup -c -w",
    "watch:cjs": "rimraf lib && FORMAT=cjs rollup -c -w",
    "clean:build": "rimraf dist es lib",
    "clean:coverage": "rimraf coverage",
    "clean": "rimraf node_modules && npm run clean:build && npm run clean:coverage"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.180",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-color": "^3.0.6",
    "@types/styled-components": "^5.1.24",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.2.5",
    "antd": "^4.19.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@antv/narrative-text-vis": "*",
    "@styled-icons/bootstrap": "^10.34.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@udecode/plate": "10.5.3",
    "lodash": "^4.17.21",
    "react-color": "^2.19.3",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "slate": "0.72.8",
    "slate-history": "0.66.0",
    "slate-hyperscript": "0.67.0",
    "slate-react": "0.74.2",
    "styled-components": "^5.3.5"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "antd": "^4.6.3"
  }
}
